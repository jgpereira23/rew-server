# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET		/listings/active           	controllers.ListingController.retrieveActiveListings 
POST 	/listings/update/:id		controllers.ListingController.update(id : Long) 
DELETE	/listings/delete/:id		controllers.ListingController.delete(id : Long)
GET 	/listings/read/:id			controllers.ListingController.read(id : Long)
POST	/app/createListing			controllers.ListingController.create